<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/dogs_in_pocket</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password"></property>
  
  <mapping class="st.dog.dip.domain.Breeder.Breeder"/>
  <mapping class="st.dog.dip.domain.Dog.Dog"/>
  <mapping class="st.dog.dip.domain.Contest.Contest"/>
  <mapping class="st.dog.dip.domain.Dogshow.Dogshow"/>
  <mapping class="st.dog.dip.domain.Expert.Expert"/>
  <mapping class="st.dog.dip.domain.Owner.Owner"/>
  <mapping class="st.dog.dip.domain.Participant.Participant"/>
  <mapping class="st.dog.dip.domain.Ring.Ring"/>
  
  </session-factory>
</hibernate-configuration>
